typedef struct student
{
    int roll;
    int marks;
    string name;

} st;
typedef union money
{
    int fees;
    int scolership;
} s1;
int main()
{
    st naitik;
    s1 money;
    money.fees = 400000;                              // now you can write only st naitik
    cout << "the fees ->" << money.fees<< endl ;    // struct student naitik;   //naitik is an Object
    naitik.roll = 545;
    naitik.marks = 88;
    naitik.name = "anuj";
    cout << "the roll->" << naitik.roll << endl
         << "the marks is->" << naitik.marks << endl
         << "the name is anuj ->" << naitik.name << endl;

    return 0;
}
